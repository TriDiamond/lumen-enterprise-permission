{
    "name": "TriDiamond/lumen-enterprise-permission",
    "description": "Enterprise Permission handling for lumen 5.5 and up",
    "keywords": [
        "TriDiamond",
        "lumen",
        "permission",
        "acl",
        "RBAC"
    ],
    "homepage": "https://github.com/TriDiamond/lumen-enterprise-permission",
    "license": "MIT",
    "authors": [
        {
            "name": "guoxiang",
            "email": "517946752@qq.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php" : ">=7.0",
        "illuminate/auth": "~5.3.0|~5.4.0|~5.5.0|~5.6.0",
        "illuminate/container": "~5.3.0|~5.4.0|~5.5.0|~5.6.0",
        "illuminate/contracts": "~5.3.0|~5.4.0|~5.5.0|~5.6.0",
        "illuminate/database": "~5.4.0|~5.5.0|~5.6.0"
    },
    "require-dev": {
        "orchestra/testbench": "~3.3.0|~3.4.2|^3.5.0",
        "phpunit/phpunit" : "^5.7|6.2|^7.0"
    },
    "autoload": {
        "psr-4": {
            "Spatie\\Permission\\": "src"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Spatie\\Permission\\Test\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "TriDiamond\\lumen-enterprise-permission\\PermissionServiceProvider"
            ]
        }
    }
}
